{
    "name": "YOLO plugin",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["notifications", "geolocation"],
    "background": {
      "scripts": ["my_background.js"],
      "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "images/get_started16.png",
          "32": "images/get_started32.png",
          "48": "images/get_started48.png"
      }
      },
    "manifest_version": 2
  }